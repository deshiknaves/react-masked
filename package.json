{
  "name": "react-masked",
  "version": "0.0.1-alpha",
  "main": "dist/react-masked.cjs.js",
  "module": "dist/react-masked.esm.js",
  "types": "dist/types/index.d.ts",
  "description": "Really light and flexible input mask in React",
  "scripts": {
    "start": "snowpack dev",
    "build:types": "tsc --emitDeclarationOnly --noEmit false --declaration --declarationDir dist/types",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "is-ci npm \"test:coverage\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm-run-all --parallel check-types test:coverage"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "@types/react": "^16.9.35"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.1",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@snowpack/app-scripts-react": "^1.0.4",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.5.3",
    "jest-runner-eslint": "^0.7.7",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.0",
    "rollup": "^2.13.1",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "snowpack": "^2.0.0",
    "typescript": "^3.8.0"
  },
  "keywords": [
    "react",
    "input",
    "mask"
  ],
  "author": "Deshiknaves",
  "license": "MIT"
}
