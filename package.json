{
  "name": "react-masked",
  "version": "0.0.1-alpha",
  "main": "index.js",
  "description": "Really light and flexible input mask in React",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "test": "is-ci npm \"test:coverage\"",
